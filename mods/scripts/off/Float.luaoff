local timeshit = 0;
function onUpdate()
    doTweenY('CamFloatShit', 'camHUD', (math.sin(timeshit*2)*20), 0.001, 'linear')
    doTweenAngle('CamAngleShit', 'camHUD', (math.sin(timeshit*5)*1), 0.001, 'linear')
    doTweenX('CamXShit', 'camHUD', (math.sin(timeshit*5)*20), 0.001, 'linear')
    timeshit = timeshit+0.01
end